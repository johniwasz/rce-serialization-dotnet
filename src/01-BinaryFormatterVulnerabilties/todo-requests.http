@hostname=localhost
@port=44355

### List all items

GET https://{{hostname}}:{{port}}/api/TodoItems HTTP/1.1

### create a new task


POST https://{{hostname}}:{{port}}/api/TodoItems HTTP/1.1
content-type: application/json

{
  "name": "walk dog"
}

### update an existing task

PUT https://{{hostname}}:{{port}}/api/TodoItems/0 HTTP/1.1
content-type: application/json

{
  "id": 0,
  "name": "walk dog",
  "isComplete": true
}

### send benign binary data

POST https://{{hostname}}:{{port}}/api/TodoItems HTTP/1.1
content-type: application/json

{
  "name": "process datatable",
  "metadata":
    [
        {
          "name": "datatable",
          "data": ""
        }
    ]
}

### send malicious binary data

POST https://{{hostname}}:{{port}}/api/TodoItems HTTP/1.1
content-type: application/json

{
  "name": "pwn the host",
  "metadata":
    [
        {
          "name": "baddata",
          "data": ""
        }
    ]
}

